{
  "extends": "solhint:all",
  "plugins": ["prettier"],
  "rules": {
      "prettier/prettier": "error",
      "func-visibility": ["warn",{"ignoreConstructors":true}],
      "avoid-throw": "off",
      "avoid-suicide": "error",
      "avoid-sha3": "warn",
      "compiler-version": ["error","^0.8.0"]
  }
}